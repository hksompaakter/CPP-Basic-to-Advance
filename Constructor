#include<iostream>
#include<conio.h>
using namespace std;
class student
{
    public:
        int id;
        double gpa;
        void display()
        {
            cout<<id<<" "<<gpa<<endl;
        }
        student(int x,double y)
        {
           id=x;
           gpa=y;
        }

};

int main()
{
    student sompa(100,3.89);
    sompa.display();

    student nishat(200,3.87);
    sompa.display();

    getch();
}
