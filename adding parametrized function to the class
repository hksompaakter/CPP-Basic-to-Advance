#include<iostream>
#include<conio.h>
using namespace std;
class student
{
    public:
        int id;
        double gpa;
        void display()
        {
            cout<<id<<" "<<gpa<<endl;
        }
        void setValue(int x,double y)
        {
            id=x;
            gpa=y;
        }
};

int main()
{
   student sompa,nishat;
   sompa.setValue(100,3.89);
   sompa.display();

   nishat.setValue(200,3.89);
   nishat.display();
   getch();
}

