#include<iostream>
#include<conio.h>
using namespace std;
class student
{
    public:
        int id;
        double gpa;
        void display()
        {
            cout<<id<<" "<<gpa<<endl;
        }

};

int main()
{
   student Sompa,Nishat;
   Sompa.id=200;
   Sompa.gpa=3.89;
   Sompa.display();

   Nishat.id=100;
   Nishat.gpa=3.87;
   Nishat.display();
   getch();
}

